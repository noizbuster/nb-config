{
  "name": "default.js",
  "version": "0.0.3",
  "description": "simple default or user configuration manager",
  "main": "default.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/defaultjs.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/index.js dist/defaultjs.min.js",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint src test examples",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:cov": "babel-node ./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha -- --recursive",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noizbuster/default.js.git"
  },
  "keywords": [
    "default",
    "configuration",
    "config"
  ],
  "author": "Noizbuster <noizbuster@noizbuster.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/noizbuster/default.js/issues"
  },
  "homepage": "https://github.com/noizbuster/default.js#readme",
  "dependencies": {
    "bluebird": "^3.4.7",
    "fs-extra": "^0.30.0",
    "js-yaml": "^3.6.1",
    "lodash": "^4.17.2"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "cross-env": "^1.0.7",
    "eslint": "^2.5.3",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.14"
  },
  "directories": {
    "test": "tests"
  }
}
